{
  "name": "plaisio/console-stratum",
  "description": "PhpPlaisio: Stratum",
  "keywords": [
    "PhpPlaisio",
    "Console",
    "Stratum"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-dom": "*",
    "plaisio/console": "^3.1.0",
    "plaisio/kernel": "^3.2.0",
    "setbased/error-handler": "^1.3.0",
    "setbased/exception": "^2.3.0",
    "setbased/helper-cast": "^3.0.0",
    "setbased/typed-config": "^2.0.0",
    "symfony/console": "^6.0|^7.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Console\\Stratum\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Console\\Stratum\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
